# Codeforces-Ladder-A
//https://codeforces.com/problemset/problem/58/A

#include<bits/stdc++.h>
using namespace std;
int main(){
string s;
cin>>s;
string c="hello";
int x=0,y=0;
for(int i=0;i<s.size();i++){
    if(s[i]==c[x]){
    x++;
    y++;
}
}
if(y==5)
cout<<"YES";
else
cout<<"NO";
return 0;
}
