# Codeforces

//https://codeforces.com/contest/1395/problem/B

#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main() {
    
     ll n,m,x,y;
        cin>>n>>m>>x>>y;
    
        for(ll i=0;i<n;i++){
          for(ll j=0;j<m;j++){
          	cout<<x<<" "<<y<<"\n";
          	y++;
          	if(y>m)
          	y=1;
          }
          x++;
          y--;
          if(y==0)
          y=m;
          if(x>n)
          x=1;
        }
    
	return 0;
}
