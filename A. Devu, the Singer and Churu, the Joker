# Codeforces
//https://codeforces.com/contest/439/problem/A

#include <iostream>
using namespace std;
int main()
{
    
    
    int n, elm, d;
	cin >> n >> d;
	int sum = 0;
	for (int i = 0; i < n; ++i) {
		cin >> elm;
		sum += elm;
	}
	if (d < (sum + (n - 1) * 10))
		cout << "-1";
	else {
		cout << (d - sum) / 5;
	}
	return 0;
}
