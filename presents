# Codeforces-Ladder-A
//http://codeforces.com/problemset/problem/136/A

#include<bits/stdc++.h>
using namespace std;
int main(){
int n;
cin>>n;
int a[n];
int p[n];
int j;
for(int i=1;i<=n;i++)
cin>>a[i];
for(int i=1;i<=n;i++){
    j=a[i];
    p[j]=i;
}
for(int i=1;i<=n;i++){
  cout<<p[i]<<" ";
}
return 0;
}
