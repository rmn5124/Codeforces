# Codeforces
//https://codeforces.com/contest/1480/problem/A

#include<bits/stdc++.h>
using namespace std;
#define tst               long t;cin>>t;while(t--)
#define rep(i,a,n)         for(int i=a;i<n;i++)
#define ull               unsigned long long int
#define ll                long long int
 
int main(){
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);cout.tie(NULL);
 tst{
   string s;cin>>s;
  rep(i,0,s.size()){
      if(1&i){
          if(s[i]=='z')
          s[i]='y';
          else 
          s[i]='z';
      }
      else{
          if(s[i]=='a')
          s[i]='b';
          else 
          s[i]='a';
      }
  }
  rep(i,0,s.size()) cout<<s[i];
  cout<<endl;
 }
return 0;   
}
