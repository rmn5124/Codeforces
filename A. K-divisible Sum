# Codeforces
//https://codeforces.com/contest/1476/problem/A

#include<bits/stdc++.h>
using namespace std;
#define ll long long int

int main(){
    ll t;
    cin>>t;
    while(t--){
        ll n,k;
        cin>>n>>k;
        if(n==1){
            cout<<k<<endl;
            continue;
        }
       else if(n%k==0){
            cout<<"1"<<endl;
        }
        else{
            if(n>k){
               if(n%k==0){cout<<"1"<<endl;}
	        else{cout<<"2"<<endl;}
	    }
            else 
              cout<<ceil((double)k/n)<<endl;
        }
	  } 
return 0;
}
