# Codeforces-Ladder-A
//https://codeforces.com/problemset/problem/118/A

#include<bits/stdc++.h>
using namespace std;
int main(){
    string a,b;
    cin>>a;
   int n=a.size();

    for(int i=0;i<n;i++){
    if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U'||a[i]=='y'||a[i]=='Y')
        continue;
        else{
           b=b+'.';
           b+=tolower(a[i]);
}
}
cout<<b;
return 0;
}
