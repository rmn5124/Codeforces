//https://codeforces.com/contest/1433/problem/A

#include<bits/stdc++.h>
using namespace std;
int main()
{   int t;
cin>>t;
while(t--){

    int x;
    cin>>x;
    int y=x%10;
    int res=10*(y-1);
    int u=0;
    while(x>0){
        u++;
        x=x/10;
    }
    res=res+(u*(u+1))/2;
    cout<<res<<endl;
}
    return 0;

}
