int main(){
    tst {
    	int n;cin>>n;
    	
   if(n==1){
      cout<<"9"<<endl;
    }
    else {
    	ll k=9;
    	cout<<"98";
    	for(ll i=0;i<n-2;i++){
    		cout<<k%10;
    		k++;
    	}
    	cout<<endl;
    }
    }
    return 0;
}
